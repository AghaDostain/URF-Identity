@using Identity.Resources
@model Identity.UI.ViewModels.Account.UnassignedExternalLoginViewModel


@{
    ViewBag.Title = AccountResources.UnassignedExternalLogin_Title;
}


<h2>@ViewBag.Title</h2>


<p class="text-danger">
    @Html.Raw(String.Format(
        AccountResources.UnassignedExternalLogin_Message,
        Model.Provider,
        Url.Action("Login", "Account"),
        Url.Action("Register", "Account")))
</p>